{
    "name": "fmark",
    "displayName": "FMark",
    "description": "FMark plugin",
    "version": "0.2.1",
    "publisher": "fmark",
    "repository": "https://github.com/ymherklotz/FMark-vscode",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onLanguage:fmark",
        "onCommand:fmark.openPreview"
    ],
    "main": "./src/extension",
    "license": "MIT",
    "contributes": {
        "commands": [
            {
                "command": "fmark.openPreview",
                "title": "FMark: Open Preview"
            },
            {
                "command": "fmark.makehtml",
                "title": "FMark: Generate HTML File"
            }
        ],
        "keybindings": [
            {
                "command": "fmark.openPreview",
                "key": "shift+ctrl+m"
            }
        ],
        "languages": [
            {
                "id": "fmark",
                "aliases": [
                    "FMark",
                    "fmark"
                ],
                "extensions": [
                    ".fmark"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fmark",
                "scopeName": "source.fmark",
                "path": "./syntaxes/fmark.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "eslint": "^4.11.0",
        "js-beautify": "^1.7.5",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    }
}